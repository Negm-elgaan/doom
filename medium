#include <iostream>
#include <stdlib.h>

using namespace std;
int main()
{
    int x,i,a,b,c;
    cin>>x;
    int arr[x];
    for (int i = 0 ; i < x ; i++)
    {
        cin>>a>>b>>c;
        if((a > c && a < b) || (a < c && a > b))
        {
            cout<<a<<endl;
        }
        else if((b > a && b < c) || (b > c && b < a))
        {
            cout<<b<<endl;
        }
        else
        {
            cout<<c<<endl;
        }
    }
    return (0);
}

