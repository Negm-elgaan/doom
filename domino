#include <iostream>

using namespace std;

int main()
{
    short x,y,a=1;
    cin>>x>>y;
    for(int i=0; i<(x*y); i++){
        if(a<(x*y)){
            a+=2;
        }else if(a==(x*y)){
        break;
        }else if(a>(x*y)){
        a/2;
        break;
        }
    }
    cout<<a/2;
    return 0;
}

