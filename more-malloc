#include<bits/stdc++.h>
#include<iomanip>
using namespace std;
int dM(int *ptr)
{
    int n;
    cin>>n;
    ptr = (int *) malloc(sizeof(*ptr)*n);
    for (int i = 0 ; i < n ; i++)
    {
        cin>>ptr[i];
    }
    for (int i = 0 ; i < n ; i++)
    {
        cout<<ptr[i];
    }
    free (ptr);
}
int main()
{
    int x;
    cout<<dM(&x);
    return 0;
}

