#include <iostream>

using namespace std;
int printword()
{
    cout << "\n" ;
    string word = "" ;
    for (char i = 65 ; i <= 90 ; i++)
        for (char j = 65 ; j <= 90 ; j++)
            for (char k = 65 ; k <= 90 ; k++)
            {
                word += i ;
                word += j ;
                word += k ;
                cout << word << endl ;
                word = "" ;
            }

}
int main()
{
    printword();
    return 0;
}

