#include <bits/stdc++.h>

using namespace std;

int main()
{
    string s;
    cin>>s;
    for (int i = 0 ; s[i] != '\0' ; i++)
    {
        if (s[i] == 'a' || s[i] == 'e' || s[i] == 'u' || s[i] == 'i' || s[i] == 'o' || s[i] == 'A' || s[i] == 'E' || s[i] == 'U' || s[i] == 'I' || s[i] == 'O' || s[i] == 'y' || s[i] == 'Y')
        {
            continue;
        }
        else
        {
            if (s[i] >= 65 && s[i] < 97)
            {
                s[i] = s[i] + 32;
                cout<<"."<<s[i];
            }
            else
                cout<<"."<<s[i];
        }
    }

    return 0;
}

