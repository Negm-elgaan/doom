#include <bits/stdc++.h>

using namespace std;
int fun(int x)
{
    int f;
    char b;
    for (int i = 0 ; i < x ; i++)
        {
            cin>>f>>b;
            for (int j = 0 ; j < f ; j++)
            {
                cout<<b<<" ";
            }
            cout<<"\n";
        }
}
int main()
{
   int n;
   cin>>n;
   fun(n);

    return 0;
}

