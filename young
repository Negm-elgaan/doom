#include <iostream>

using namespace std;

int main()
{
    short n,a = 0,b = 0,c = 0,i,j;
    cin>>n;
    short v[n][n];
    for (i = 0 ; i < n ; i++)
    {
        for (j = 0 ; j < 3 ; j++)
        {
            cin>>v[i][j];
        }
        a += v[i][0];
        b += v[i][1];
        c += v[i][2];
    }
    if (a == 0 && b == 0 && c == 0)
        cout<<"YES\n";
    else
        cout<<"NO\n";
    return 0;
}

