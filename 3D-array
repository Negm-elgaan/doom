#include <bits/stdc++.h>

using namespace std;

int main()
{
    int x,y,z,sum;
    cin>>x>>y>>z;
    int a[x][y][z];
    for(int i=0; i<x; i++)
    {
        for(int j=0; j<y; j++)
        {
                for(int l=0; l<z; l++)
                {
                  cin>>a[i][j][l];
                }
        }
    }
     for(int i=0; i<x; i++)
     {
        for(int j=0; j<y; j++)
        {
            for(int l=0; l<z; l++)
                {
                    cout<<a[i][j][l];
                }
        }
     }
    return 0;
}

