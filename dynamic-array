#include <iostream>
using namespace std;
int main()
{
    float* ptr;
    int n = 0 ;
    cin >> n;
    ptr = new float [n];
    for (int i = 0; i < n; i++)
    {
        cin >> *(ptr + i);
    }
    for (int i = 0; i < n; i++)
    {
        cout << *(ptr + i) << endl;
    }
    delete[] ptr;
    
}
